    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'mixer'",
            "cargo": {
                "args": [
                    "build",
                    "--example=mixer_demo"
                ],
                "filter": {
                    "name": "mixer_demo",
                    "kind": "example"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },

        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'audio_ex1'",
            "cargo": {
                "args": [
                    "build",
                    "--example=audio_ex1",
                    "--features=extras"
                ],
                "filter": {
                    "name": "audio_ex1",
                    "kind": "example"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },

        
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'g_lue'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=g_lue",
                    "--package=g_lue"
                ],
                "filter": {
                    "name": "g_lue",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'g_lue'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=g_lue",
                    "--package=g_lue"
                ],
                "filter": {
                    "name": "g_lue",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}